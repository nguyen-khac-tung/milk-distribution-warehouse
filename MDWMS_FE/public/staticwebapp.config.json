{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif}", 
      "/css/*", 
      "/static/*", 
      "/*.{js,css,png,jpg,gif,ico,svg,woff,woff2,ttf,eot}"
    ]
  },
  "routes": [
    {
      "route": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    },
    {
      "route": "/",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}